{
  "name": "angular-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve -o --port 4201",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-dist-server": "npm run build --prod && http-server dist/angular-boilerplate/ -p 8080",
    "compodoc": "npx compodoc",
    "storybook:docs": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run storybook:docs && start-storybook -p 6006 src/assets",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.3",
    "@angular/cdk": "^11.0.1",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/forms": "~11.0.3",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/router": "~11.0.3",
    "@angular/service-worker": "~11.0.3",
    "@ngrx/effects": "^10.1.0",
    "@ngrx/store": "^10.1.0",
    "@ngrx/store-devtools": "^10.1.0",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.6.3",
    "tinycolor2": "^1.4.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.3",
    "@angular/cli": "~11.0.3",
    "@angular/compiler-cli": "~11.0.3",
    "@angular/language-service": "^11.0.3",
    "@babel/core": "^7.10.3",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-storysource": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/angular": "^6.1.10",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/tinycolor2": "^1.4.2",
    "babel-loader": "^8.1.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
